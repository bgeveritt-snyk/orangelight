<header role="banner">
  <div class="l-branding__pul">
    <div class="container">
      <%= link_to root_path, rel: 'home', title: 'Princeton University Library', class: 'logo__pul' do %>
         <%= image_tag("library_full_logo.svg", class: "logo-svg") %>
         <%= image_tag("library_full_logo.png", class: "logo-no-svg") %>
      <% end %>
      <div class="pull-right menu--level-1">
        <ul>
          <li><a href="http://library.princeton.edu">Library Website</a></li>
          <li><a href="http://library.princeton.edu/help/contact-us">Contact Us</a></li>
        </ul>
      </div>
    </div>
  </div>

  <div class="l-branding__catalog">
    <div class="container">
      <h1 class="pull-left"><a href="/">Catalog</a></h1>

       <%= render :partial=>'/user_util_links' %>
    </div>
  </div>

  <div id="search-navbar" class="navbar navbar-default navbar-static-top" role="search">
    <div class="container">
      <%= render_search_bar %>
    </div>
  </div>
</header>